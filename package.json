  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-watch": "npx babel src/js/ --watch --out-dir src/build/js/",
    "build": "npx babel src/js/ --out-dir src/build/js/",
    "start": "npx concurrently 'npm run build' 'npx http-server ./src'",
